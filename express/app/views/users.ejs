<%- include('partials/header.ejs') %>
<%- include('partials/nav.ejs') %>

 <div class="container mt-5">
    <h3>Users Table</h3>
    <table class="table table-hover">
     
        <thead class="thead-dark">
            <th>ID</th>
            <th>Name</th>
            <th>Username</th>
            <th>E-mail</th>
            <th>Phone</th>
        </thead>
        <tbody>        
        <% users.forEach((user) => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <td><%= user.phone %></td>
            </tr>

        <%})%>
        </tbody>
    </table>
</div>
    <%- include('partials/footer.ejs') %>
